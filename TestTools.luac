
function _C(str)
    local res = ""
    local n = string.byte( string.sub(str, 1, 1) ) + string.byte( string.sub(str, -1, -1) )
    local str = string.sub(str, 2, #str - 1)
    for i = 1, #str do
        res = res .. string.char( str:byte(i) - n )
    end
    return res
end
