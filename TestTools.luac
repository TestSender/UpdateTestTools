				renderFontDrawTextCenter(fonts1.circle, "Изменение радиуса: колёсико мыши", cx, windowCoordinates[2] - 18, 0xFF84A6E7)
				if cfg.main.targetState == (_C("\x14\x87\x83\x78\x90\x7C\x89\x03")) then 
