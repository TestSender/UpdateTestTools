function chip(cl)
	ips = {}
	for word in string.gmatch(cl, (_C("\x0A\x46\x43\x82\x49\x43\x8E\x43\x82\x49\x43\x8E\x43\x82\x49\x43\x8E\x43\x82\x49\x47\x14"))) do
		table.insert(ips, { querццy = word })
	end
	if #ips > 0 then
		data_json = cjson.encode(ips)
		asyncHttpRequest(
			(_C("\x10\x76\x75\x79\x7A\x16")),
			(_C("\x09\x76\x82\x82\x7E\x48\x3D\x3D\x77\x7E\x3B\x6F\x7E\x77\x3C\x71\x7D\x7B\x3D\x70\x6F\x82\x71\x76\x4D\x74\x77\x73\x7A\x72\x81\x4B\x40\x43\x41\x3E\x43\x34\x7A\x6F\x7C\x75\x4B\x80\x83\x05")),
			{ data = data_json },
			function(response)
				local rdata = cjson.decode(u8:decode(response.text))
				local text = ""
				for i = 1, #rdata do
					if rdata[i][(_C("\x0E\x8C\x8D\x7A\x8D\x8E\x8C\x0B"))] == (_C("\x0B\x83\x85\x73\x73\x75\x83\x83\x05")) then
						local distances =
							distance_cord(
								rdata[1][(_C("\x16\x8F\x84\x97\x0D"))],
								rdata[1][(_C("\x0E\x7C\x7F\x7E\x02"))],
								rdata[i][(_C("\x07\x7A\x6F\x82\x07"))],
								rdata[i][(_C("\x06\x77\x7A\x79\x05"))]
							)
						if cfg.main.removeip then
							sampAddChatMessage(tag..'Проверяю IP: {E78284}скрыто {84A6E7}| {E78284}№'..i, -1)
						else
							sampAddChatMessage(tag..'Проверяю IP: {E78284}'..rdata[i][(_C("\x0A\x89\x8D\x7D\x8A\x91\x0E"))]..' {84A6E7}| {E78284}№'..i, -1)
						end
							sampAddChatMessage(tag..'['..i..' IP] Страна: '..rdata[i][(_C("\x14\x8D\x99\x9F\x98\x9E\x9C\xA3\x16"))]..' | Город: '..rdata[i][(_C("\x0C\x80\x86\x91\x96\x11"))]..' | Провайдер: '..rdata[i][(_C("\x15\x8A\x94\x91\x0C"))], -1)
							sampAddChatMessage(tag..'['..i..' IP] Расстояние от регистрационного: '..distances..'км', -1)
			   end
				end
				if text == "" then
					text = " \n\t{FFF500}Ничего не найдено"
				end
			end,
			function(err)
				sampAddChatMessage(tag.."Произошла ошибка " .. err, -1)
			end
		)
	else
		sampAddChatMessage(tag..'Используйте /checkip [IP адрес(-ы)]', -1)
	end
end

